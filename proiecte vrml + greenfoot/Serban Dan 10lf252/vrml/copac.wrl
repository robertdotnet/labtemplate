#VRML V2.0 utf8
WorldInfo {
   title "ModelCopac"
  
}





#trunchi

Transform {
   translation -4 0 0
   children [
      Shape {
         appearance Appearance {
			texture ImageTexture {
				url "trunchi.jpg"
				repeatS TRUE
				repeatT TRUE
				}
            material Material {
               diffuseColor 255 255 255
            }
         }
         geometry Cone {
            bottomRadius 0.5
            height 10
            bottom FALSE
         }
      }
   ]
}



